{
  "name": "express-app-es6-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "concurrently \"tsc -w\" \"cross-env NODE_ENV=development nodemon dist/index.js\"",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "argon2": "^0.27.0",
    "aws-sdk": "^2.774.0",
    "body-parser": "^1.19.0",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.0",
    "express-fileupload": "^1.2.0",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.1",
    "ioredis": "^4.19.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5",
    "spotify-web-api-node": "^5.0.2",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "yarn dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/connect-redis": "^0.0.14",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.3",
    "@types/ioredis": "^4.17.6",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.3",
    "@types/redis": "^2.8.28",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "kill-port": "^1.6.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
